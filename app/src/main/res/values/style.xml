<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CalendarDateTextStyle">
        <item name="android:font">@font/pretendard_bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="CalendarHeaderTextStyle">
        <item name="android:font">@font/pretendard_bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/texthigh</item>
    </style>

    <style name="CalendarWeekdayTextStyle">
        <item name="android:font">@font/pretendard_medium</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/texthigh</item>
    </style>

    <style name="tabTextSizeBold" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/pretendard</item>
    </style>
</resources>